
termux-setup-storage 

apt update

apt install ruby -y

apt install figlet -y 

apt install openssl -y

gem install lolcat

cd

cd ../usr/etc

rm motd bash.bashrc profile

cd 

mv $HOME/termux-fs/bash.bashrc /data/data/com.termux/files/usr/etc

mv $HOME/termux-fs/profile /data/data/com.termux/files/usr/etc

exit
